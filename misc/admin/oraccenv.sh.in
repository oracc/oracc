#!/bin/sh
umask 026
ORACC=@@ORACC@@
ORACC_BUILDS=@@ORACC_BUILDS@@
ORACC_HOME=@@ORACC_HOME@@
ORACC_HOST=@@ORACC_HOST@@
ORACC_MODE=@@ORACC_MODE@@
ORACC_USER=@@ORACC_USER@@
export ORACC ORACC_HOME ORACC_HOST ORACC_BUILDS ORACC_USER ORACC_MODE
TEXINPUTS=$TEXINPUTS:@@ORACC_BUILDS@@/lib/tex/:.
TEXFORMATS=@@ORACC_BUILDS@@/lib/tex/:.
TEXFONTS=@@ORACC_BUILDS@@/lib/tex:@@ORACC_BUILDS@@/lib/tex/fonts:.
export TEXINPUTS TEXFORMATS TEXFONTS
PATH=$PATH:$HOME/00bin:$ORACC/bin:@@TeXLive@@
export PATH
