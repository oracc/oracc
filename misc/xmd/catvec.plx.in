#!/usr/bin/perl
use warnings; use strict;
my $vec = '';
open(P,"/var/local/oracc/pub/cdli/cat/pqids.lst");
while (<P>) {
    s/^cdli:P// || next; chomp;
#    warn "$_\n";
    vec($vec,$_,1) = 1;
}
close(P);

#warn "vec is ", length($vec), " bytes\n";
my $vecfile = '@@ORACC@@/pub/cdli/catpnums.vec';
open(V,">$vecfile");
print V $vec;
close(V);

system 'chmod', 'o+r', $vecfile;
1;
